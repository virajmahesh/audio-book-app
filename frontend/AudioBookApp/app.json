{
  "expo": {
    "name": "Aurelius",
    "slug": "aurelius-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "./assets/fonts/**"
    ],
    "android": {
      "package": "com.aurelius.app",
      "permissions": [
        "LOCATION", "CAMERA_ROLL"
      ]
    },
    "description": "",
    "androidStatusBar": {
      "backgroundColor": "#ffffff"
    }
  }
}
